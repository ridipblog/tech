                <div class="col-md-6 form-group contact_form">
                  <input type="text" name="name" class="form-control" id="name" value="{{lookup input 0}}" placeholder="Your Name" required style="background:#1b1d26;color: white;">
                </div>
                <div class="col-md-6 form-group mt-3 mt-md-0" style="background:#1b1d26;">
                  <input type="email" class="form-control " name="email" id="email" value="{{lookup input 1}}" placeholder="Your Email" required style="background:#1b1d26;color: white;">
                </div>
                <p></p>
                <div class="form-group mt-3 mt-md-0" style="background:#1b1d26;">
                  <textarea class="form-control" name="message" rows="5" value="{{lookup input 2}}" placeholder="Message" required style="background:#1b1d26;color: white;"></textarea>
                </div>
                <p></p>
                <div class="form-group mt-3 mt-md-0" style="background:#1b1d26;">
                  <p style="border-bottom:2px solid white;display: {{lookup style1 0}};color:{{lookup style1 1}}">{{lookup style1 2}}</p>
                </div>
              </div>
              <div class="text-center" style="background:#1b1d26;"><input type="submit" value="Submit" style="width:70%;background: transparent;color:white;border:2px solid white"></div>
            </form>
